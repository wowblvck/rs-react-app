{
  "name": "rs-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "browserslist": [
    "last 2 versions",
    "not dead"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "ts-node ./server.ts",
    "build": "npm run clean && npm run build:client && npm run build:server && npm run build:server-mode",
    "build:client": "tsc && vite build --outDir ./dist/client",
    "build:server": "tsc && vite build --config vite.server.config.ts --ssr ./src/entry-server.tsx",
    "build:server-mode": "tsc -b tsconfig.node.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "clean": "rimraf dist && rimraf instrumented && rimraf coverage && rimraf .nyc_output",
    "preview": "vite preview",
    "lint": "npx eslint src && npx eslint __TESTS__",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check && npx prettier __TESTS__ --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "vitest",
    "test:prep": "npm run test:clean && npx nyc instrument --compact=false src instrumented",
    "test:e2e": "npm run test:prep && npm run cypress:run && npx nyc report --reporter=text-summary",
    "test:clean": "rimraf coverage && rimraf .nyc_output && rimraf instrumented",
    "coverage": "npm run test:e2e && vitest run --coverage",
    "coverage:unit": "vitest run --coverage",
    "coverage:e2e": "npm run test:e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-redux": "^7.1.26",
    "@types/react-transition-group": "^4.4.6",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "cypress-real-events": "^1.10.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "moment": "^2.29.4",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-transition-group": "^4.4.5",
    "redux-persist": "^6.0.0",
    "rimraf": "^5.0.1",
    "serve-static": "^1.15.0",
    "vite-plugin-babel-macros": "^1.0.6",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.11.0",
    "@hookform/devtools": "^4.3.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-istanbul": "^0.34.3",
    "@vitest/coverage-v8": "^0.34.3",
    "babel-plugin-istanbul": "^6.1.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.4",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "msw": "^1.2.5",
    "node-fetch": "^3.3.2",
    "nyc": "^15.1.0",
    "prettier": "3.0.3",
    "sass": "^1.66.1",
    "sharp": "^0.32.5",
    "source-map-support": "^0.5.21",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-gh-pages": "^0.4.1",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-istanbul": "^5.0.0",
    "vitest": "^0.34.3"
  }
}
